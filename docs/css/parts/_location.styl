#map2
  width: 100vw
  height: calc(100vh - 60px - 70px)

.addPoint
  position: fixed
  bottom : 100px
  right: 10px
  width: 50px
  height: 50px
  border-radius: 100%
  background: main-red
  display: flex
  justify-content: center
  align-items: center
  box-shadow: 0px 6px 6px -3px rgba(black, 0.2)
  &:focus
    color: white
    text-decoration: none
  i
    color: white
    font-size: 30px